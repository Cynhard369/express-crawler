{
  "rules": {
    "indent": 2,
    "no-unused-vars": 2,
    "no-alert": 2,
    "quotes": [1, "single"],
    "semi": [2, "always", {"omitLastInOneLineBlock": true}]
  }, 
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
